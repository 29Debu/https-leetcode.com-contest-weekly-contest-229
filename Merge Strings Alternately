#include <iostream>
#include<string.h>
using namespace std;
int main()
{
   string s1 = "ktj";
   string s2 = "springfest";
   string s = "";
   for (int i = 0; i < s1.length() || i < s2.length(); ++i)
   {
   	if(i < s1.length())
   		s += s1[i];
   	if(i < s2.length())
   		s += s2[i];
   }
   cout<<s;
}
